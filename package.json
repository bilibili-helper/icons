    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x"
  },
  "devDependencies": {
    "tsdx": "0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bilibili-helper/reactIcons.git"
  },
  "keywords": [
    "bilibilihelper",
    "icons"
  ],
  "bugs": {
    "url": "https://github.com/bilibili-helper/reactIcons/issues"
  },
  "homepage": "https://github.com/bilibili-helper/reactIcons#readme"
