{
  "name": "@bilibilihelper/reactIcons",
  "private": false,
  "version": "2.0.0",
  "license": "MIT",
  "description": "Radix UI React Icon Set",
  "authors": [
    "DrowsyFlesh <jjj201200@gmail.com>"
  ],
  "module": "dist/reactIcons.esm.js",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "files": [
    "outline/",
    "solid/",
    "dist/",
    "index.js",
    "manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsdx build --format es,cjs --entry src/index.tsx",
